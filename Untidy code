#include <iostream>
#include <string>

using namespace std;
int main() {
	int number = 3;
	cout << "This is untidy code." << "I'm surprised it works." << endl;
	cout << "It has" <<	number << "lines of code - " << "each more hideous than the last. ";
	cout << "You probably should add some line breaks in the text too" << endl;
	cin.get(); //keeps console window open in Visual Studio
	return 0;
}
